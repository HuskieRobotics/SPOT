    "name": "finalActionOccurrence",
    "outputPath": "finalStage",
    "options": {
      "actionArrayPath": "teleopFilter",
      "ids": ["onstage", "park", "spotlit", "unpark", "onstageFall"],
      "default": {
        "id": "noStage",
        "ts": 0
      }
    }
  },
  {
    "type": "tmp",
    "name": "map",
    "outputPath": "scores.stageExclusive",
    "options": {
      "path": "finalStage.id",
      "map": {
        "noStage": 0,
        "onstage": 3,
        "park": 1,
        "spotlit": 4,
        "unpark": 0,
        "onstageFall": 1
      }
    }
  },
  {
    "type": "tmp",
    "name": "map",
    "outputPath": "stage.noStage",
    "options": {
      "path": "finalStage.id",
      "map": {
        "noStage": 1,
        "onstage": 0,
        "park": 0,
        "spotlit": 0,
        "unpark": 0,
        "onstageFall": 0
      }
    }
  },
  {
    "type": "tmp",
    "name": "map",
    "outputPath": "stage.onstage",
    "options": {
      "path": "finalStage.id",
      "map": {
        "noStage": 0,
        "onstage": 1,
        "park": 0,
        "spotlit": 0,
        "unpark": 0,
        "onstageFall": 0
