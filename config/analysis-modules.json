[
  {
    "view": "team",
    "module": "PerformanceTimePlot",
    "name": "Performance By Each Match",
    "position": "main",
    "options": {
      "trackedStats": [
        "scores.auto",
        "scores.teleop",
        "scores.endgame",
        "scores.total"
      ]
    }
  },
  {
    "view": "team",
    "module": "RadarChart",
    "name": "Ratings By Time Intervals",
    "position": "main",
    "options": {
      "sections": [
        {
          "name": "Auto"
        },
        {
          "name": "Transition"
        },
        {
          "name": "Active 1"
        },
        {
          "name": "Inactive 1"
        },
        {
          "name": "Active 2"
        },
        {
          "name": "Inactive 2"
        },
        {
          "name": "Endgame"
        }
      ],
      "lines": [
        {
          "name": "Shooting",
          "times": [
            {
              "name": "Auto",
              "path": "averageScores.autoShootingRating"
            },
            {
              "name": "Transition",
              "path": "averageScores.teleopTransitionShootingRating"
            },
            {
              "name": "Active 1",
              "path": "averageScores.activeShift1ShootingRating"
            },
            { "name": "Inactive 1", "path": "none" },
            {
              "name": "Active 2",
              "path": "averageScores.activeShift2ShootingRating"
            },
            { "name": "Inactive 2", "path": "none" },
            {
              "name": "Endgame",
              "path": "averageScores.endgameShootingRating"
            }
          ]
        },
        {
          "name": "Collecting",
          "times": [
            {
              "name": "Auto",
              "path": "averageScores.autoCollectingRating"
            },
            {
              "name": "Transition",
              "path": "averageScores.teleopTransitionCollectingRating"
            },
            {
              "name": "Active 1",
              "path": "averageScores.activeShift1CollectingRating"
            },
            {
              "name": "Inactive 1",
              "path": "averageScores.inactive1CollectingRating"
            },
            {
              "name": "Active 2",
              "path": "averageScores.activeShift2CollectingRating"
            },
            {
              "name": "Inactive 2",
              "path": "averageScores.inactive2CollectingRating"
            },
            {
              "name": "Endgame",
              "path": "averageScores.endgameCollectingRating"
            }
          ]
        },
        {
          "name": "Passing",
          "times": [
            {
              "name": "Auto",
              "path": "averageScores.autoPassingRating"
            },
            {
              "name": "Transition",
              "path": "averageScores.teleopTransitionPassingRating"
            },
            {
              "name": "Active 1",
              "path": "averageScores.activeShift1PassingRating"
            },
            {
              "name": "Inactive 1",
              "path": "averageScores.inactive1PassingRating"
            },
            {
              "name": "Active 2",
              "path": "averageScores.activeShift2PassingRating"
            },
            {
              "name": "Inactive 2",
              "path": "averageScores.inactive2PassingRating"
            },
            {
              "name": "Endgame",
              "path": "averageScores.endgamePassingRating"
            }
          ]
        },
        {
          "name": "Defense",
          "times": [
            { "name": "Auto", "path": "averageScores.autoDefenseScore" },
            {
              "name": "Transition",
              "path": "averageScores.teleopTransitionDefenseScore"
            },
            {
              "name": "Active 1",
              "path": "averageScores.activeShift1DefenseScore"
            },
            {
              "name": "Inactive 1",
              "path": "averageScores.inactive1DefenseScore"
            },
            {
              "name": "Active 2",
              "path": "averageScores.activeShift2DefenseScore"
            },
            {
              "name": "Inactive 2",
              "path": "averageScores.inactive2DefenseScore"
            },
            {
              "name": "Endgame",
              "path": "averageScores.endgameDefenseScore"
            }
          ]
        }
      ]
    }
  },

  {
    "view": "team",
    "module": "HeatmapScatterPlot",
    "name": "Auto Start and Stop Shooting Heatmap",
    "position": "main",
    "options": {
      "coordinatePath": "other.pos",
      "aggregatedActionsPath": "aggregatedActions",
      "actionGroups": [
        {
          "name": "Auto Start Shooting",
          "actions": ["autoStartShooting"]
        },
        {
          "name": "Auto End Shooting",
          "actions": ["autoStopShooting"]
        }
      ],
      "actionLabels": {
        "autoStartShooting": "Auto Start Shooting Position",
        "autoStopShooting": "Auto End Shooting Position"
      },
      "imgPath": "img/field.svg"
    }
  },
  {
    "view": "team",
    "module": "HeatmapScatterPlot",
    "name": "Teleop Shooting Heatmap",
    "position": "main",
    "options": {
      "coordinatePath": "other.pos",
      "aggregatedActionsPath": "aggregatedActions",
      "actionGroups": [
        { "name": "Teleop Start Shooting", "actions": ["startShoot"] }
      ],
      "actionLabels": { "startShoot": "Teleop Start Shooting Position" },
      "imgPath": "img/field.svg"
    }
  },
  {
    "view": "team",
    "module": "Bar",
    "name": "Attempted Climb Level",
    "position": "main",
    "options": {
      "bars": [
        {
          "name": "Fall",
          "path": "counts.fall"
        },
        {
          "name": "L1",
          "path": "counts.attemptL1"
        },

        {
          "name": "L2",
          "path": "counts.attemptL2"
        },
        {
          "name": "L3",
          "path": "counts.attemptL3"
        }
      ]
    }
  },
  {
    "view": "team",
    "module": "Pie",
    "name": "Pre-Loads",
    "position": "main",
    "options": {
      "slices": [
        {
          "name": "Pre-Loaded Fuel",
          "path": "counts.preloadFuel"
        },
        {
          "name": "Pre-Load None",
          "path": "counts.preloadNone"
        }
      ]
    }
  },
  {
    "view": "team",
    "module": "Stats",
    "position": "side",
    "name": "Team Stats",
    "options": {
      "list": [
        {
          "name": "Average Passing Rating",
          "path": "averagesScores.passingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Defense Rating",
          "path": "averageScores.defenseRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Shooting Rating",
          "path": "averageScores.shootingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Collection Rating",
          "path": "averageScores.collectingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Auto Shooting Rating",
          "path": "averageScores.autoShootingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Auto Passing Rating",
          "path": "averageScores.autoPassingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Auto Shooting Time",
          "path": "cycles.autoShootingTime.averageTime",
          "multiplier": 0.001,
          "decimals": 2,
          "hideIfValue": 0,
          "sort": -1,
          "unit": "s"
        },
        {
          "name": "Average Teleop Shooting Time",
          "path": "cycles.shootingTime.averageTime",
          "multiplier": 0.001,
          "decimals": 2,
          "hideIfValue": 0,
          "sort": -1,
          "unit": "s"
        },
        {
          "name": "Average Climb Time",
          "path": "averageTimes.attemptClimbTime",
          "multiplier": 0.001,
          "decimals": 2,
          "sort": -1,
          "unit": "s"
        }
      ]
    }
  },

  {
    "view": "match",
    "module": "SingleDisplay",
    "name": "Alliance Score",
    "position": "main",
    "options": {
      "path": "averageScores.total",
      "aggrMethod": "sum",
      "decimals": 2
    }
  },
  {
    "view": "match",
    "module": "ColumnDisplay",
    "name": "Average Shooting Time",
    "position": "main",
    "options": [
      {
        "name": "Average Auto Shooting Time",
        "path": "cycles.autoShootingTime.averageTime",
        "multiplier": 0.001,
        "decimals": 2,
        "hideIfValue": 0,
        "sort": -1,
        "unit": "s"
      },
      {
        "name": "Average Teleop Shooting Time ",
        "path": "cycles.shootingTime.averageTime",
        "multiplier": 0.001,
        "decimals": 2,
        "hideIfValue": 0,
        "sort": -1,
        "unit": "s"
      }
    ]
  },
  {
    "view": "match",
    "module": "Stats",
    "position": "side",
    "name": "Alliance Stats",
    "options": {
      "list": [
        {
          "name": "Expected Auto Points",
          "path": "averageScores.auto",
          "decimals": 2,
          "aggrMethod": "sum"
        },
        {
          "name": "Expected Teleop Points",
          "path": "averageScores.teleop",
          "decimals": 2,
          "aggrMethod": "sum"
        },
        {
          "name": "Expected Barge Points",
          "path": "avgEndgamePoints",
          "decimals": 2,
          "aggrMethod": "sum"
        },
        {
          "name": "Expected Score",
          "path": "averageScores.total",
          "decimals": 2,
          "aggrMethod": "sum"
        }
      ]
    }
  }
]
