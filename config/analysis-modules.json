[
  {
    "view": "team",
    "module": "PerformanceTimePlot",
    "name": "Performance By Each Match",
    "position": "main",
    "options": {
      "trackedStats": [
        "scores.auto",
        "scores.teleop",
        "scores.endgame",
        "scores.total"
      ]
    }
  },
  {
    "view": "team",
    "module": "HeatmapScatterPlot",
    "name": "Teleop Shooting Heatmap",
    "position": "main",
    "options": {
      "coordinatePath": "other.pos",
      "aggregatedActionsPath": "aggregatedActions",
      "actionGroups": [
        { "name": "Teleop Start Shooting", "actions": ["startShoot"] }
      ],
      "actionLabels": { "startShoot": "Teleop Start Shooting Position" },
      "imgPath": "img/field.svg"
    }
  },
  {
    "view": "team",
    "module": "Bar",
    "name": "Attemped Climb Level",
    "position": "main",
    "options": {
      "bars": [
        {
          "name": "Fall",
          "path": "counts.fall"
        },
        {
          "name": "L1",
          "path": "counts.teleopAttemptL1"
        },

        {
          "name": "L2",
          "path": "counts.attemptL2"
        },
        {
          "name": "L3",
          "path": "counts.attemptL3"
        }
      ]
    }
  },
  {
    "view": "team",
    "module": "Bar",
    "name": "TBA Climb Levels",
    "position": "main",
    "options": {
      "bars": [
        {
          "name": "L1",
          "path": "counts.endGameTowerRobot_Level1"
        },

        {
          "name": "L2",
          "path": "counts.endGameTowerRobot_Level2"
        },
        {
          "name": "L3",
          "path": "counts.endGameTowerRobot_Level3"
        }
      ]
    }
  },
  {
    "view": "team",
    "module": "Pie",
    "name": "Pre-Loads",
    "position": "main",
    "options": {
      "slices": [
        {
          "name": "Pre-Loaded Fuel",
          "path": "counts.preloadFuel"
        },
        {
          "name": "Pre-Load None",
          "path": "counts.preloadNone"
        }
      ]
    }
  },
  {
    "view": "team",
    "module": "Stats",
    "position": "side",
    "name": "Team Stats",
    "options": {
      "list": [
        {
          "name": "Average Passing Rating",
          "path": "averagesScores.passingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Defense Rating",
          "path": "averageScores.defenseRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Shooting Rating",
          "path": "averageScores.shootingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Collection Rating",
          "path": "averageScores.collectingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Auto Shooting Rating",
          "path": "averageScores.autoShootingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Auto Passing Rating",
          "path": "averageScores.autoPassingRating",
          "decimals": 2,
          "sort": 1
        },
        {
          "name": "Average Auto Shooting Time",
          "path": "cycles.autoShootingTime.averageTime",
          "multiplier": 0.001,
          "decimals": 2,
          "hideIfValue": 0,
          "sort": -1,
          "unit": "s"
        },
        {
          "name": "Average Teleop Shooting Time",
          "path": "cycles.shootingTime.averageTime",
          "multiplier": 0.001,
          "decimals": 2,
          "hideIfValue": 0,
          "sort": -1,
          "unit": "s"
        },
        {
          "name": "Average Climb Time",
          "path": "averageTimes.attemptClimbTime",
          "multiplier": 0.001,
          "decimals": 2,
          "sort": -1,
          "unit": "s"
        }
      ]
    }
  },
  {
    "view": "match",
    "module": "SingleDisplay",
    "name": "Alliance Score",
    "position": "main",
    "options": {
      "path": "averageScores.total",
      "aggrMethod": "sum",
      "decimals": 2
    }
  },
  {
    "view": "match",
    "module": "ColumnDisplay",
    "name": "Auto Points",
    "position": "main",
    "options": {
      "path": "averageScores.auto",
      "sort": 1,
      "decimals": 2
    }
  },
  {
    "view": "match",
    "module": "ColumnDisplay",
    "name": "Teleop Points",
    "position": "main",
    "options": {
      "path": "averageScores.teleop",
      "sort": 1,
      "decimals": 2
    }
  },
  {
    "view": "match",
    "module": "ColumnDisplay",
    "name": "Endgame Points",
    "position": "main",
    "options": {
      "path": "averageScores.endgame",
      "sort": 1,
      "decimals": 2
    }
  },
  {
    "view": "match",
    "module": "ColumnDisplay",
    "name": "Total Points",
    "position": "main",
    "options": {
      "path": "averageScores.total",
      "sort": 1,
      "decimals": 2
    }
  },

  {
    "view": "match",
    "module": "Stats",
    "position": "main",
    "name": "Alliance Stats",
    "options": {
      "list": [
        {
          "name": "Expected Auto Points",
          "path": "averageScores.auto",
          "decimals": 2,
          "aggrMethod": "sum"
        },
        {
          "name": "Expected Teleop Points",
          "path": "averageScores.teleop",
          "decimals": 2,
          "aggrMethod": "sum"
        },
        {
          "name": "Expected Endgame Points",
          "path": "averageScores.endgame",
          "decimals": 2,
          "aggrMethod": "sum"
        },
        {
          "name": "Expected Score",
          "path": "averageScores.total",
          "decimals": 2,
          "aggrMethod": "sum"
        }
      ]
    }
  }
]
