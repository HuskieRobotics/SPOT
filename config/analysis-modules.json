                    "hideIfValue": 0,
                    "sort": -1,
                    "unit": "s to end"
                },
                {
                    "name": "Climb End Time",
                    "path": "averages.endClimbTime",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "hideIfValue": 0,
                    "sort": -1,
                    "unit": "s to end"
                },
                {
                    "name": "Climb Duration",
                    "path": "averages.climbDuration",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "hideIfValue": 0,
                    "sort": -1,
                    "unit": "s"
                },
                {
                    "name": "Time Per Ball",
                    "path": "timePerBall",
                    "multiplier": 0.001,
                    "decimals": 2,
                    "hideIfValue": 0,
                    "sort": -1,
                    "unit": "s"
                },
                {
                    "name": "Accuracy",
